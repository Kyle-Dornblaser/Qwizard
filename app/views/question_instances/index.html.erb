<h1>Listing question_instances</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_instances.each do |question_instance| %>
      <tr>
        <td><%= question_instance.question %></td>
        <td><%= link_to 'Show', question_instance %></td>
        <td><%= link_to 'Edit', edit_question_instance_path(question_instance) %></td>
        <td><%= link_to 'Destroy', question_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question instance', new_question_instance_path %>
