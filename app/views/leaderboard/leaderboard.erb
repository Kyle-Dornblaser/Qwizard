<div id="leaderboard_wrapper">
     <div id="leaderboard_main_panel">
          <center>
          <h1> Top 10 Leaderboard </h1>
          <br />
          <table class="leaderboard_contentz" cellspacing="0">
               <tr>
                    <th class="leaderboard_member_name"> Member </th>
                    <th class="leaderboard_member_point"> Points </th>
               </tr>
               
               <% generate_top_leaderboard.each do |user_rank| %>
                    <tr>
                    <td class="leaderboard_member_name"> <%= user_rank["user_name"] %> </td>
                    <td class="leaderboard_member_point"> <%= user_rank["award"] %> </td>
                    </tr>
               <% end %>
               
          </table>
          <br /><br /><br /><br />
          <h1> Top 10 This Week </h1>
          <br />
          <table class="leaderboard_contentz" cellspacing="0">
               <tr>
                    <th class="leaderboard_member_name"> Member </th>
                    <th class="leaderboard_member_point"> Points </th>
               </tr>
               
               <% generate_168_leaderboard.each do |user_rank| %>
                    <tr>
                    <td class="leaderboard_member_name"> <%= user_rank["user_name"] %> </td>
                    <td class="leaderboard_member_point"> <%= user_rank["award"] %> </td>
                    </tr>
               <% end %>
             
          </table>
          <br />
          <br />
          <br />
          <br />
          </center>
     </div>
</div>